<% title "Viewing Enquiry" %>

<% if policy(@enquiry).destroy? %>
  <div class="btn-toolbar" role="toolbar">
    <div class="btn-group">
      <%= link_to 'Delete Enquiry', @enquiry,
        data: { confirm: 'Are you sure you want to delete this enquiry?' },
        method: :delete,
        class: "btn btn-danger",
        role: "button"
      %>
    </div>
  </div>
<% end %>

<%= render partial: 'enquiry', locals: { enquiry: @enquiry } %>
